<!DOCTYPE html>
<html lang="en" ng-app="Member">
<<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="google-signin-scope" content="profile email">
    <meta name="google-signin-client_id" content="398506059135-ksocfvjtrc37jn87mhahgsa4lh7nfkbj.apps.googleusercontent.com">
    <script src="https://apis.google.com/js/platform.js" async defer></script> 
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.2/jquery.min.js"></script>
    <script src="https://apis.google.com/js/client:platform.js?onload=start" async defer></script>
    <script src="https://plus.google.com/js/client:platform.js" async defer></script>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Oishi Member</title>
    <!-- Bootstrap -->
    <link href="~/Template/vendors/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
    <!-- Font Awesome -->
    <link href="~/Template/vendors/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <!-- NProgress -->
    <link href="~/Template/vendors/nprogress/nprogress.css" rel="stylesheet">
    <!-- iCheck -->
    <link href="~/Template/vendors/iCheck/skins/flat/green.css" rel="stylesheet">
    <!-- bootstrap-progressbar -->
    <link href="~/Template/vendors/bootstrap-progressbar/css/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet">
    <!-- JQVMap -->
    <link href="~/Template/vendors/jqvmap/dist/jqvmap.min.css" rel="stylesheet" />
    <!-- Custom Theme Style -->
    <link href="~/Template/build/css/custom.min.css" rel="stylesheet">
    <link href="~/css/google-plus-signin.css" rel="stylesheet" />
    
    <script src="https://apis.google.com/js/platform.js?onload=renderButton" async defer></script>
    <script>
    function start() {
      gapi.load('auth2', function() {
        auth2 = gapi.auth2.init({
            client_id: '398506059135-ksocfvjtrc37jn87mhahgsa4lh7nfkbj.apps.googleusercontent.com',
          // Scopes to request in addition to 'profile' and 'email'
          //scope: 'additional_scope'
        });
      });
    }
    </script>

 <style>
    #head {
        color: #333;
        background-color: darkred;
        border-color: #5c5151;
    }
</style>
</head>
<body>
    <!-- page content -->
    <div id="head" class="panel panel-default" role="main" ng-controller="RegisterController">
        <div id="head" class="panel-heading ui-draggable-handle">
            <h1 class="panel-title" style="color:white" align="center"><strong>Register</strong></h1>
        </div>
        <div class="clearfix"></div>
        <div class="row">
            <div class="col-md-12 col-sm-12 col-xs-12">
                <div class="x_panel">
                    <div class="x_title">
                        <h2>Your Profile</h2>
                        <div class="clearfix"></div>
                    </div>
                    
                    <div class="x_content">
                        <form id="demo-form2" data-parsley-validate class="form-horizontal form-label-left">

                            <div class="form-group">
                                <label class="control-label col-md-3 col-sm-3 col-xs-12 form-group has-feedback">User Name</label>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <input type="text" class="form-control has-feedback-left" id="username" placeholder="username" required="required">
                                    <span class="fa fa-user form-control-feedback left" aria-hidden="true"></span>
                                </div>
                            </div>
                            
                            <div class="form-group">
                                <label class="control-label col-md-3 col-sm-3 col-xs-12 form-group has-feedback">Password</label>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <input type="password" class="form-control has-feedback-left" id="password" placeholder="password" required="required">
                                    <span class="fa fa-unlock-alt form-control-feedback left" aria-hidden="true"></span>
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="control-label col-md-3 col-sm-3 col-xs-12 form-group has-feedback">Confirm Password</label>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <input type="password" class="form-control has-feedback-left" id="confirm" placeholder="confirm password" required="required">
                                    <span class="fa fa-unlock-alt form-control-feedback left" aria-hidden="true"></span>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="control-label col-md-3 col-sm-3 col-xs-12">Title
                                    <span class="required">*</span></label>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <select class="form-control" ng-model="register.title" required="required" >
                                        <option value="">-- select your title --</option>
                                        <option value="1">Mrs.</option>
                                        <option value="2">Miss</option>
                                        <option value="3">Mr.</option>
                                    </select>
                                </div>
                            </div>
                            

                            <div class="form-group">
                                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">First Name
                                    <span class="required">*</span></label>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <input type="text" ng-model="register.first_name" id="firstname" required="required" class="form-control col-md-7 col-xs-12">
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="last-name">Middle Name</label>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <input type="text" ng-model="register.middlename" id="lastname" name="lastname" class="form-control col-md-7 col-xs-12">
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="last-name">Last Name
                                    <span class="required">*</span></label>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <input type="text" ng-model="register.last_name" id="lastname" name="lastname" required="required" class="form-control col-md-7 col-xs-12">
                                </div>
                            </div>

                            <div class="form-group">
                                <label for="gender" class="control-label col-md-3 col-sm-3 col-xs-12">Gender
                                    <span class="required">*</span></label>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <p>
                                        <input class="flat iradio_flat-green" ng-model="register.gender" value="male" type="radio" name="gender" id="genderM"  checked="" required /> Male
                                        <input class="flat iradio_flat-green" ng-model="register.gender" value="female" type="radio" name="gender" id="genderF" /> Female
                                    </p>
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="address">Address1
                                    <span class="required">*</span></label>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <input type="text" ng-model="register.address1" id="address" name="address" required="required" class="form-control col-md-7 col-xs-12">
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="address">Address2</label>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <input type="text" ng-model="register.address2" id="address" name="address" class="form-control col-md-7 col-xs-12">
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="control-label col-md-3 col-sm-3 col-xs-12">City
                                    <span class="required">*</span></label>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <select class="form-control" ng-model="register.city" required="required">
                                        <option value="">--select your city--</option>
                                        <option value="1">Bangkok</option>
                                        <option value="2">Chiang Mai</option>
                                        <option value="3">Krabi</option>
                                        <option value="4">Phuket</option>
                                        <option value="5">Ubon Ratchathani</option>
                                    </select>
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="zipcode">Zip code
                                    <span class="required">*</span></label>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <input type="number" ng-model="register.zipcode" id="zipcode" name="zipcode" required="required" class="form-control col-md-7 col-xs-12">
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="phone">Phone Number
                                    <span class="required">*</span></label>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <input type="text" ng-model="register.phone" id="phone" name="phone" required="required" class="form-control col-md-7 col-xs-12">
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="phone">Mobile Number</label>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <input type="text" ng-model="register.mobile" id="phone" name="phone" class="form-control col-md-7 col-xs-12">
                                </div>
                            </div>



                            <div class="form-group">
                                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="phone">Email
                                    <span class="required">*</span></label>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <input type="email" ng-model="register.email"  class="form-control has-feedback-left" id="email" placeholder="example@email.com" required="required">
                                    <span class="fa fa-envelope form-control-feedback left" aria-hidden="true"></span>
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="control-label col-md-3 col-sm-3 col-xs-12">Birthday
                                    <span class="required">*</span></label>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <div class="col-xs-12" style="padding-left: 0px;padding-right: 0px;">
                                        <div class="control-group">
                                            <div class="input-group">
                                                <input type="text" ng-model="register.birthday" class="form-control datepicker ng-pristine ng-valid ng-isolate-scope ng-not-empty ng-valid-date ng-valid-required ng-touched" ng-click="openStart()" min-date="minStartDate" max-date="maxStartDate" uib-datepicker-popup="dd-MM-yyyy" ng-model="start" is-open="popupStart.opened" datepicker-options="dateOptions" ng-required="true" close-text="Close" show-button-bar="false" required="required" />
                                                <span class="input-group-addon">
                                                    <i class="ace-icon fa fa-calendar"></i>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="phone">Your picture</label>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <div class="input-group">
                                        <br>
                                        <img id="blah" ng-src="{{register.image}}" class="img-circle img-responsive center-block" style="width:50%">
                                        <br><br>
                                        <input type="file" class="align:center" ng-model="register.picture" onchange="readURL(this);">
                                    </div>
                                </div>
                            </div>

                            <div class="ln_solid"></div>
                            <div class="form-group">
                                <div class="col-md-6 col-sm-6 col-xs-12 col-md-offset-3">
                                    <button type="button" ng-click="Cancel(register)"
                                            class="btn btn-primary pull-right">
                                        Cancel
                                    </button>
                                    <button type="submit" ng-click="addMember(register)" 
                                            class="btn btn-success pull-right">Submit</button>
                                </div>
                            </div>
                        </form>

                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
</html>



<script src="~/Scripts/angular.js"></script>
<script src="~/Scripts/angular-route.min.js"></script>
<script src="~/Scripts/angular-sanitize.js"></script>
<script src="~/Scripts/angular-translate.js"></script>
<script src="~/MemberJS/app.js"></script>
<script src="~/Template/vendors/jquery/dist/jquery.min.js"></script>
<script src="~/MemberJS/register.js"></script>
<script src="~/MemberJS/picture.js"></script>
<script src="~/Scripts/google-plus-signin.js"></script>
<script data-require="ui-bootstrap@1.0.0" data-semver="1.0.0" src="https://cdnjs.cloudflare.com/ajax/libs/angular-ui-bootstrap/1.0.0/ui-bootstrap-tpls.js"></script>




