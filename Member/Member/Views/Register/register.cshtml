<!DOCTYPE html>
<html lang="en" ng-app="Member">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Oishi Member</title>
    <!-- Bootstrap -->
    <link href="~/Template/vendors/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
    <!-- Font Awesome -->
    <link href="~/Template/vendors/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <!-- NProgress -->
    <link href="~/Template/vendors/nprogress/nprogress.css" rel="stylesheet">
    <!-- iCheck -->
    <link href="~/Template/vendors/iCheck/skins/flat/green.css" rel="stylesheet">
    <!-- bootstrap-progressbar -->
    <link href="~/Template/vendors/bootstrap-progressbar/css/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet">
    <!-- JQVMap -->
    <link href="~/Template/vendors/jqvmap/dist/jqvmap.min.css" rel="stylesheet" />
    <!-- Custom Theme Style -->
    <link href="~/Template/build/css/custom.min.css" rel="stylesheet">
<style>
    #head {
        color: #333;
        background-color: darkred;
        border-color: #5c5151;
    }
</style>
</head>
<body>
    <!-- page content -->
    <div id="head" class="panel panel-default" role="main" ng-controller="RegisterController">
        <div id="head" class="panel-heading ui-draggable-handle">
            <h1 class="panel-title" style="color:white" align="center"><strong>Register</strong></h1>
        </div>
        <div class="clearfix"></div>
        <div class="row">
            <div class="col-md-12 col-sm-12 col-xs-12">
                <div class="x_panel">
                    <div class="x_title">
                        <h2>Your Profile</h2>
                        <div class="clearfix"></div>
                    </div>
                    <div class="x_content">
                        <form id="demo-form2" data-parsley-validate class="form-horizontal form-label-left">

                            @*<div class="form-group">
                                <label class="control-label col-md-3 col-sm-3 col-xs-12 form-group has-feedback">User Name</label>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <input type="text" class="form-control has-feedback-left" id="username" placeholder="username">
                                    <span class="fa fa-user form-control-feedback left" aria-hidden="true"></span>
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="control-label col-md-3 col-sm-3 col-xs-12 form-group has-feedback">Password</label>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <input type="password" class="form-control has-feedback-left" id="password" placeholder="password">
                                    <span class="fa fa-unlock-alt form-control-feedback left" aria-hidden="true"></span>
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="control-label col-md-3 col-sm-3 col-xs-12 form-group has-feedback">Confirm Password</label>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <input type="password" class="form-control has-feedback-left" id="confirm" placeholder="confirm password">
                                    <span class="fa fa-unlock-alt form-control-feedback left" aria-hidden="true"></span>
                                </div>
                            </div>*@
                            <div class="form-group">
                                <label class="control-label col-md-3 col-sm-3 col-xs-12">Title</label>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <select class="form-control" ng-model="register.title">
                                        <option value="">-- กรุณาเลือกคำนำหน้า --</option>
                                        <option value="1">นาย</option>
                                        <option value="2">นางสาว</option>
                                        <option value="3">นาง</option>
                                        <option value="4">เด็กชาย</option>
                                        <option value="5">เด็กหญิง</option>
                                    </select>
                                </div>
                            </div>
                            

                            <div class="form-group">
                                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">First Name</label>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <input type="text" ng-model="register.firstname" id="firstname" required="required" class="form-control col-md-7 col-xs-12">
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="last-name">Middle Name</label>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <input type="text" ng-model="register.middlename" id="lastname" name="lastname" required="required" class="form-control col-md-7 col-xs-12">
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="last-name">Last Name</label>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <input type="text" ng-model="register.lastname" id="lastname" name="lastname" required="required" class="form-control col-md-7 col-xs-12">
                                </div>
                            </div>

                            <div class="form-group">
                                <label for="gender" class="control-label col-md-3 col-sm-3 col-xs-12">Gender</label>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <p>
                                        <input class="flat iradio_flat-green" ng-model="register.gender" value="M" type="radio" name="gender" id="genderM"  checked="" required /> Male
                                        <input class="flat iradio_flat-green" ng-model="register.gender" value="F" type="radio" name="gender" id="genderF" /> Female
                                    </p>
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="address">Address1</label>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <input type="text" ng-model="register.address1" id="address" name="address" required="required" class="form-control col-md-7 col-xs-12">
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="address">Address2</label>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <input type="text" ng-model="register.address2" id="address" name="address" required="required" class="form-control col-md-7 col-xs-12">
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="control-label col-md-3 col-sm-3 col-xs-12">City</label>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <select class="form-control" ng-model="city">
                                        <option value="">--กรุณาเลือกจังหวัด--</option>
                                        <option value="BKK">Bangkok</option>
                                        <option value="CHM">Chiang Mai</option>
                                        <option value="KBI">Krabi</option>
                                        <option value="HKT">Phuket</option>
                                        <option value="URT">Ubon Ratchathani</option>
                                    </select>
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="zipcode">Zip code</label>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <input type="number" ng-model="register.zipcode" id="zipcode" name="zipcode" required="required" class="form-control col-md-7 col-xs-12">
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="phone">Phone Number</label>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <input type="text" ng-model="register.phone" id="phone" name="phone" required="required" class="form-control col-md-7 col-xs-12">
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="phone">Mobile Number</label>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <input type="text" ng-model="register.mobile" id="phone" name="phone" required="required" class="form-control col-md-7 col-xs-12">
                                </div>
                            </div>



                            <div class="form-group">
                                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="phone">Email</label>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <input type="email" ng-model="register.email"  class="form-control has-feedback-left" id="email" placeholder="example@email.com">
                                    <span class="fa fa-envelope form-control-feedback left" aria-hidden="true"></span>
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="control-label col-md-3 col-sm-3 col-xs-12">Birthday</label>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <div class="col-xs-12" style="padding-left: 0px;padding-right: 0px;">
                                        <div class="control-group">
                                            <div class="input-group">
                                                <input type="text" ng-model="register.birthday" class="form-control datepicker ng-pristine ng-valid ng-isolate-scope ng-not-empty ng-valid-date ng-valid-required ng-touched" ng-click="openStart()" min-date="minStartDate" max-date="maxStartDate" uib-datepicker-popup="dd-MM-yyyy" ng-model="start" is-open="popupStart.opened" datepicker-options="dateOptions" ng-required="true" close-text="Close" show-button-bar="false" required="required" />
                                                <span class="input-group-addon">
                                                    <i class="ace-icon fa fa-calendar"></i>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            @*<div class="form-group">
                                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="phone">Your picture</label>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <div class="input-group">
                                        <br>
                                        <img id="blah" src="~/Images/register/user_sm.png" class="img-circle img-responsive center-block" style="width:50%">
                                        <br><br>
                                        <input type="file" class="align:center" onchange="readURL(this);">
                                    </div>
                                </div>
                            </div>*@

                            <div class="ln_solid"></div>
                            <div class="form-group">
                                <div class="col-md-6 col-sm-6 col-xs-12 col-md-offset-3">
                                    <a href="/Login/login" class="btn btn-primary pull-right">Cancel</a>
                                    <button type="submit" ng-click="addMember(register)" 
                                            class="btn btn-success pull-right">Submit</button>
                                </div>
                            </div>
                        </form>

                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
</html>



<script src="~/Scripts/angular.js"></script>
<script src="~/Scripts/angular-route.min.js"></script>
<script src="~/Scripts/angular-sanitize.js"></script>
<script src="~/MemberJS/app.js"></script>
<script src="~/Template/vendors/jquery/dist/jquery.min.js"></script>
<script src="~/MemberJS/register.js"></script>
<script src="~/MemberJS/picture.js"></script>
<script data-require="ui-bootstrap@1.0.0" data-semver="1.0.0" src="https://cdnjs.cloudflare.com/ajax/libs/angular-ui-bootstrap/1.0.0/ui-bootstrap-tpls.js"></script>




